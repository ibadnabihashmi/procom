doctype html
html(lang='en')
    head
        meta(charset='utf-8')
        title Procom - MMXVI
        meta(name='viewport', content='width=device-width, initial-scale=1, maximum-scale=1')
        // Le styles
        link(href='assets/css/bootstrap.css', rel='stylesheet')
        link(href='assets/css/bootstrap-responsive.css', rel='stylesheet')
        link(href='assets/css/typography.css', rel='stylesheet')
        link(href='stylesheets/supersized.css', rel='stylesheet')
        link(href='stylesheets/supersized.shutter.css', rel='stylesheet')
        link(href='stylesheets/jquery.tweet.css', rel='stylesheet')
        link(href='stylesheets/flexslider.css', rel='stylesheet')
        // Isotope
        link(rel='stylesheet', href='stylesheets/prettyPhoto.css')
        link(rel='stylesheet', href='stylesheets/isotope.css')
        // Custom style
        link(href='stylesheets/icomoon.css', rel='stylesheet')
        link(href='stylesheets/responsive-nav.css', rel='stylesheet')
        link(href='stylesheets/style.css', rel='stylesheet')
    body
        #fb-root
        script.
            (function(d, s, id) {
            var js, fjs = d.getElementsByTagName(s)[0];
            if (d.getElementById(id)) return;
            js = d.createElement(s); js.id = id;
            js.src = "//connect.facebook.net/en_US/sdk.js#xfbml=1&version=v2.5&appId=887877234622664";
            fjs.parentNode.insertBefore(js, fjs);
            }(document, 'script', 'facebook-jssdk'));
        header#mobile-header.hidden-desktop.clearfix
            #nav
                ul.clearfix
                    li
                        a.scroll-link(href='/#header', data-soffset='0') Home
                    li
                        a.scroll-link(href='/#about', data-soffset='0') About
                    li
                        a.scroll-link(href='/#registration', data-soffset='0') registration
                    li
                        a.scroll-link(href='/#sponsors', data-soffset='0') sponsors
                    li
                        a.scroll-link(href='/#ambassador', data-soffset='0') ambassador
                    li
                        a.scroll-link(href='/#contact', data-soffset='0') Contact
        .header-fixed-nav.hidden-phone.hidden-tablet
            .nav
                img(src='images/logo-xs.png', style='width: 40px;margin-top: 20px;')
                ul
                    li
                        a#header-linker.scroll-link.active(href='/#header', data-soffset='0') home
                    li
                        a#about-linker.scroll-link(href='/#about', data-soffset='0') about
                    li
                        a#registration-linker.scroll-link(href='/#registration', data-soffset='0') registration
                    li
                        a#sponsors-linker.scroll-link(href='/#sponsors', data-soffset='0') sponsors
                    li
                        a#sponsors-linker.scroll-link(href='/#ambassador', data-soffset='0') ambassador
                    li
                        a#contact-linker.scroll-link(href='/#contact', data-soffset='0') contact
            .nav2
                div
                    a.facebook-icon(href='#')
                div
                    a.twitter-icon(href='#')
        .page
            .container
                .row(style='margin-top: 45px;')
                    .span7.offset2
                        h1.text-center #{head}
                hr
                .row(style='margin-top: 20px;')
                    .span12
                        select.comptype
                            option(value="all") all
                            option(value="motivationalTalk") motivationalTalk
                            option(value="adWars") adWars
                            option(value="entreeWars") entreeWars
                            option(value="marketing") marketing
                            option(value="i2p") i2p
                            option(value="cdc") cdc
                            option(value="microcontroller") microcontroller
                            option(value="hardwareExhibition") hardwareExhibition
                            option(value="networkDesign") networkDesign
                            option(value="autocad") autocad
                            option(value="roboRace") roboRace
                            option(value="roboWars") roboWars
                            option(value="soldering") soldering
                            option(value="spc") spc
                            option(value="speedMobileApp") speedMobileApp
                            option(value="speedWebDev") speedWebDev
                            option(value="softwareExhibition") softwareExhibition
                            option(value="debugging") debugging
                            option(value="databaseDesign") databaseDesign
                            option(value="uiux") uiux
                            option(value="codeInDark") codeInDark
                            option(value="gameplayDesign") gameplayDesign
                            option(value="hackathon") hackathon
                            option(value="creativeWriting") creativeWriting
                            option(value="digitalArt") digitalArt
                            option(value="iqTest") iqTest
                            option(value="iquest") iquest
                            option(value="painting") painting
                            option(value="photography") photography
                            option(value="shortFilming") shortFilming
                            option(value="sketching") sketching
                            option(value="spellingBee") spellingBee
                            option(value="nfs") nfs
                            option(value="counterStrike") counterStrike
                            option(value="dota") dota
                            option(value="fifa") fifa

                .row(style='margin-top: 20px;')
                    .span11
                        input#token(type='hidden',name='_csrf',value=_csrf)
                        .resultcont
                            h3.lengthpart Total : #{length}
                            -for(var i=0;i<data.length;i++){
                            div(style="padding: 10px; border: 1px solid #B6B6B6;")
                                p
                                    strong Email:
                                    |  #{data[i].email}
                                p
                                    strong Phone:
                                    |  #{data[i].phone}
                                p
                                    strong Member1:
                                    |  #{data[i].mem1}
                                p
                                    strong Member2:
                                    |  #{data[i].mem2}
                                p
                                    strong Member3:
                                    |  #{data[i].mem3}
                                p
                                    strong Member4:
                                    |  #{data[i].mem4}
                                p
                                    strong Member5:
                                    |  #{data[i].mem5}
                                p
                                    strong Team name:
                                    |  #{data[i].teamname}
                                p
                                    strong Ambassador:
                                    |  #{data[i].ambassador}
                                p
                                    strong Institute:
                                    |  #{data[i].institute}
                                p
                                    strong Why?:
                                    |  #{data[i].why}
                                p
                                    strong Software:
                                    |  #{data[i].software}
                                p
                                    strong type:
                                    |  #{data[i].type}
                            -}
        #footer
            section.container-fluid
                .row-fluid
                    section.container
                        .row
                            article.span4
                                .copyright
                                    p Copyright Â© 2016 procom.net
                            // // End of .span12
                            article.span8
                                .credits.text-right
                                    p Procom ,expanding possibilities
                            // // End of .span12
                        // // End of .row
                    // // End of .container
                // // End of .row-fluid
            // // End of .container-fluid
        // // End of #footer
